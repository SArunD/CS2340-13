<AnchorPane fx:id="anchorPane" prefHeight="475.0" prefWidth="850.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project.codenames.controllers.CNEndScreenController">
   <children>
      <ImageView fx:id="backgroundIV" fitHeight="475.0" fitWidth="850.0" pickOnBounds="true">
